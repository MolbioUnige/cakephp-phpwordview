{
    "name": "molbio-unige/php-word-view",
    "description": "PhpWordView plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "phpoffice/phpword": "^0.18.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "PhpWordView\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpWordView\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
